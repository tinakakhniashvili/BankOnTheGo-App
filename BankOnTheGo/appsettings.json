{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=YOUR_SERVER;Initial Catalog=YOUR_DATABASE;User ID=YOUR_USER;Password=YOUR_PASSWORD;TrustServerCertificate=True;Persist Security Info=True;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "EmailConfiguration": {
    "From": "YOUR_EMAIL",
    "SmtpServer": "YOUR_SMTP_SERVER",
    "Port": 465,
    "Username": "YOUR_EMAIL_USERNAME",
    "Password": "YOUR_EMAIL_PASSWORD"
  },
  "JWT": {
    "ValidAudience": "YOUR_AUDIENCE_URL",
    "ValidIssuer": "YOUR_ISSUER_URL",
    "Secret": "YOUR_SECRET_KEY"
  },
  "AllowedHosts": "*"
}
